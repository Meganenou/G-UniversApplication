<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.g_universapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".injection.G_UniversApplication"
        android:allowBackup="true"
        android:icon="@drawable/code_ic"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.GUniversApplication">
        <activity android:name=".presentation.main.AccountActivity" />
        <activity android:name=".presentation.main.Description" />
        <activity
            android:name=".presentation.main.GameActivity"
            android:label="@string/title_activity_game"
            android:theme="@style/Theme.GUniversApplication.NoActionBar" />
        <activity android:name=".presentation.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>